          <div class="input-group-append" data-target='#start_timegteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-clock-o"></i></div>
          </div>
          <script type="text/javascript">
            $(function () {
                $('#start_timegteqPicker').datetimepicker({
                  format: 'LT'
                });
            });
          </script>
        </div>
      </div>
      <div class='col-md-6'>
        <div class='input-group date' id='start_timelteqPicker' data-target-input="nearest">
          <%= f.text_field :start_time_lteq, :class => "form-control datetimepicker-input", :placeholder => "Start time less than or equal to", :data => {:target => '#start_timelteqPicker'} %>
          <div class="input-group-append" data-target='#start_timelteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-clock-o"></i></div>
          </div>
          <script type="text/javascript">
            $(function () {
                $('#start_timelteqPicker').datetimepicker({
                  format: 'LT'
                });
            });
          </script>
        </div>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :start_date, "Start date" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='start_dategteqPicker' data-target-input="nearest">
          <%= f.text_field :start_date_gteq, :class => "form-control datetimepicker-input", :placeholder => "Start date greater than or equal to", :data => {:target => '#start_dategteqPicker'} %>
          <div class="input-group-append" data-target='#start_dategteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>

      <div class='col-md-6'>
        <div class='input-group date' id='start_datelteqPicker' data-target-input="nearest">
          <%= f.text_field :start_date_lteq, :class => "form-control datetimepicker-input", :placeholder => "Start date less than or equal to", :data => {:target => '#start_datelteqPicker'} %>
          <div class="input-group-append" data-target='#start_datelteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
      $(function () {
          $('#start_dategteqPicker').datetimepicker({
              format: 'MM/DD/YYYY'
          });
          $('#start_datelteqPicker').datetimepicker({
