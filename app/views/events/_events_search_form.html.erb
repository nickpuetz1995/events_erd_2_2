<%= search_form_for @q, :class => collapse_search_form, :id => "events_filters" do |f| %>
  <p class="lead">Narrow results:</p>

  <div class="form-group">
    <%= f.label :name_cont, "Name contains" %>
    <%= f.text_field :name_cont, :class => "form-control", :placeholder => "Name contains" %>
  </div>

  <div class="form-group">
    <%= f.label :end_time, "End time" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='end_timegteqPicker' data-target-input="nearest">
          <%= f.text_field :end_time_gteq, :class => "form-control datetimepicker-input", :placeholder => "End time greater than or equal to", :data => {:target => '#end_timegteqPicker'} %>
          <div class="input-group-append" data-target='#end_timegteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-clock-o"></i></div>
          </div>
          <script type="text/javascript">
            $(function () {
                $('#end_timegteqPicker').datetimepicker({
                  format: 'LT'
                });
            });
          </script>
        </div>
      </div>
      <div class='col-md-6'>
        <div class='input-group date' id='end_timelteqPicker' data-target-input="nearest">
          <%= f.text_field :end_time_lteq, :class => "form-control datetimepicker-input", :placeholder => "End time less than or equal to", :data => {:target => '#end_timelteqPicker'} %>
          <div class="input-group-append" data-target='#end_timelteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-clock-o"></i></div>
          </div>
          <script type="text/javascript">
            $(function () {
                $('#end_timelteqPicker').datetimepicker({
                  format: 'LT'
                });
            });
          </script>
        </div>
      </div>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :end_date, "End date" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='end_dategteqPicker' data-target-input="nearest">
          <%= f.text_field :end_date_gteq, :class => "form-control datetimepicker-input", :placeholder => "End date greater than or equal to", :data => {:target => '#end_dategteqPicker'} %>
          <div class="input-group-append" data-target='#end_dategteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>

      <div class='col-md-6'>
        <div class='input-group date' id='end_datelteqPicker' data-target-input="nearest">
          <%= f.text_field :end_date_lteq, :class => "form-control datetimepicker-input", :placeholder => "End date less than or equal to", :data => {:target => '#end_datelteqPicker'} %>
          <div class="input-group-append" data-target='#end_datelteqPicker' data-toggle="datetimepicker">
            <div class="input-group-text"><i class="fa fa-calendar"></i></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script type="text/javascript">
      $(function () {
          $('#end_dategteqPicker').datetimepicker({
              format: 'MM/DD/YYYY'
          });
          $('#end_datelteqPicker').datetimepicker({
              useCurrent: false,
              format: 'MM/DD/YYYY'
          });
          $('#end_dategteqPicker').on("dp.change", function (e) {
              $('#end_datelteqPicker').data("DateTimePicker").minDate(e.date);
          });
          $('#end_datelteqPicker').on("dp.change", function (e) {
              $('#end_dategteqPicker').data("DateTimePicker").maxDate(e.date);
          });
      });
  </script>

  <div class="form-group">
    <%= f.label :start_time, "Start time" %>
    <div class="row">
      <div class='col-md-6 mb-1'>
        <div class='input-group date' id='start_timegteqPicker' data-target-input="nearest">
